   "source": [
    "\n",
    "from typing import List\n",
    "\n",
    "class Solution:\n",
    "    def maxProfit(self, prices: List[int]) -> int:\n",
    "        max_profit = 0\n",
    "        for i in range(1,len(prices)):\n",
    "            if prices[i] - prices[i-1] > 0:\n",
    "                max_profit += prices[i] - prices[i-1]\n",
    "        return max_profit\n"
   ]
