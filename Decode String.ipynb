   "source": [
    "class Solution:\n",
    "    def decodeString(self, s: str) -> str:\n",
    "        import re\n",
    "\n",
    "        pattern = r'(\\d+)\\[([a-zA-Z]*)\\]'\n",
    "        \n",
    "        while '[' in s:\n",
    "            s = re.sub(pattern, lambda m: int(m.group(1)) * m.group(2), s)\n",
    "        return s"
   ]
