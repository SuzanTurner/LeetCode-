   "source": [
    "class Solution:\n",
    "    def strStr(self, haystack: str, needle: str) -> int:\n",
    "        if needle in haystack:\n",
    "            return haystack.find(needle)\n",
    "        return -1\n",
    "\n",
    "sol = Solution()\n",
    "haystack = \"Leetcode\"\n",
    "needle = \"et\"\n",
    "result = sol.strStr(haystack,needle)\n",
    "print(result)"
   ]
